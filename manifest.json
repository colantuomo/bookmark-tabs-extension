{
    "manifest_version": 2,
    "name": "Bookmark Tabs",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "./assets/icon.png",
        "default_popup": "./src/popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks"
    ]
}